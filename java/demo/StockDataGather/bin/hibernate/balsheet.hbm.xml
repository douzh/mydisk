<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.iteedu.stock.spider.xueqiu.bean.TBalSheet" table="T_BALSHEET">
    <id name="cid" type="string">
      <column name="C_ID"/>
      <generator class="assigned" />
    </id>
    <property name="symbol" type="string">
      <column name="symbol" />
    </property>
    <property name="code" type="string">
      <column name="code" />
    </property>
    <property name="reportDateStr" type="string">
      <column name="reportdatestr"/>
    </property>
    <property name="reportDate" type="java.util.Date">
      <column name="reportdate"/>
    </property>
    <property name="comptype" type="string">
      <column name="comptype"/>
    </property>
    <property name="orgData" type="string">
      <column name="orgdata"/>
    </property>
  </class>
</hibernate-mapping>

